#include<stdio.h>

int main(void) {
	char array[2][3][20];
	int n, i, j;
	for (n = 0; n < 2; n++); {
		printf("\n 학생 %d의 이름 : ", n + 1);
		gets(array[n][0]);
		printf("\n 학생 %d의 학과 : ", n + 1);
		gets(array[n][1]);
		printf("\n 학생 %d의 학번 : ", n + 1);
		gets(array[n][2]);
	}
	for (n = 0; n < 2; n++) {
		printf("\n\n 학생%d", n + 1);
		for (i = 0; i < 3; i++) {
			printf("\n\t");
			for (j = 0; array[n][i][j]!="\0"; j++) {
				printf("%c", array[n][i][j]);
			}
		}
	}
	getchar();
}
